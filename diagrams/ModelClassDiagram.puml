@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model as ModelPackage <<Rectangle>>{
Class "<<interface>>\nReadOnlyModulePan" as ReadOnlyModulePlan
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nModel" as Model
Class ModulePlan
Class ModelManager
Class UserPrefs

Class UniqueModuleList
Class Module
Class ModuleDescription
Class Grade
Class ModuleCode
Class ModuleName
Class Year
Class Semester

Class I #FFFFFF
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

ModulePlan .up.|> ReadOnlyModulePlan

ModelManager .up.|> Model
Model .right.> ReadOnlyUserPrefs
Model .left.> ReadOnlyModulePlan
ModelManager -left-> "1" ModulePlan
ModelManager -right-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

ModulePlan *--> "1" UniqueModuleList
UniqueModuleList --> "~* all" Module
Module *--> "1" ModuleCode
Module *--> "1" ModuleName
Module *--> "1" Grade
Module *--> "1" ModuleDescription
Module *--> "1" Year
Module *--> "1" Semester

Module -[hidden]up--> I
UniqueModuleList -[hidden]right-> I

ModuleCode -[hidden]right-> ModuleName
ModuleName -[hidden]right-> ModuleDescription
ModuleDescription -[hidden]right-> Grade

ModelManager --> "~* filtered" Module
@enduml
